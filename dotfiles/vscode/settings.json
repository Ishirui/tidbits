{
  "git.autofetch": true,
  "github.gitProtocol": "ssh",
  "githubPullRequests.notifications": "pullRequests",
  "terminal.external.linuxExec": "konsole",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
